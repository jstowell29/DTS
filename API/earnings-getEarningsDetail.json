    {
      "fieldname": "distId",
      "type": "Numeric",
      "notes": "Distributor ID Number"
    },
    {
      "fieldname": "checkType",
      "type": "String",
      "notes": "PB for Primary Bonus SB for Secondary Bonus MA and MB are Manual checks Default is PB"
    },
    {
      "fieldname": "pvDate",
      "type": "Numeric",
      "notes": "Volume Period date YYYYPP"
    },
    {
      "fieldname": "transactionId",
      "type": "Numeric",
      "notes": "Earnings Transaction ID taken from Earnings Summary service"
    }
