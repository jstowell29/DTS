    {
      "fieldname": "dst_city",
      "type": "String",
      "notes": "City, State Zip"
    },
    {
      "fieldname": "dst_country_code",
      "type": "String",
      "notes": "3 Letter Country code"
    },
    {
      "fieldname": "dst_postal_code",
      "type": "String",
      "notes": "Postal Code"
