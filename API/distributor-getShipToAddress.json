    {
      "fieldname": "addr_2",
      "type": "String",
      "notes": "Apartment or Suite Number"
    },
    {
      "fieldname": "addr_3",
      "type": "String",
      "notes": "City, State Postal Code"
    },
    {
      "fieldname": "addr_4",
