{
  "id": "ftAdmin-singleFileInfo",
  "name": "FtAdmin.singleFileInfo",
  "description": "Returns information about the specified file imported to FastTree",
  "inputs": [
    {
      "fieldname": "file",
      "type": "String",
      "notes": "FastTree File Name (e.g.,$CURR, TRDSTGEN, TRPB201405)"
    }
  ],
  "outputs": [
    {
      "fieldname": "begin",
      "type": "Numeric",
      "notes": "Begin Date of period"
    },
    {
      "fieldname": "end",
      "type": "Numeric",
      "notes": "End date of period"
    },
    {
      "fieldname": "import",
      "type": "Numeric",
      "notes": "Import Date of File"
    },
    {
      "fieldname": "name",
      "type": "String",
      "notes": "Name of File (e.g. TRPB201305)"
    },
    {
      "fieldname": "period",
      "type": "Numeric",
      "notes": "Volume Period Number"
    },
    {
      "fieldname": "periodType",
      "type": "String",
      "notes": "Volume Period Type (e.g. PB = Primary Bonus)"
    },
    {
      "fieldname": "periodYear",
      "type": "Numeric",
      "notes": "Volume Period Year"
    }
  ]
}