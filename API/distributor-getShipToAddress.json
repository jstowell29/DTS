    {
      "fieldname": "key",
      "type": "String",
      "notes": "Ship-to description"
    },
    {
      "fieldname": "num",
      "type": "Numeric",
      "notes": "Number"
    },
    {
      "fieldname": "phone_1",
      "type": "Numeric",
      "notes": "Phone 1 Field"
