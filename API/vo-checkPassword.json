{
  "id": "vo-checkPassword",
  "name": "Vo.checkPassword",
  "description": "Checks if an Evo username and password combination is valid",
  "inputs": [
    {
      "fieldname": "username",
      "type": "String",
      "notes": "Username/Distributor ID"
    },
    {
      "fieldname": "password",
      "type": "String",
      "notes": "Password"
    }
  ],
  "outputs": [
    {
      "fieldname": "status",
      "type": "String",
      "notes": "Returns OK if successful"
    }
  ]
}